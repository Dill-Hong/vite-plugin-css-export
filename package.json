{
  "name": "vite-plugin-css-export",
  "author": {
    "name": "shixuanhong"
  },
  "description": "A Vite plugin for sharing variables between Javascript and CSS (or Sass, Less, etc.)",
  "version": "0.3.0",
  "keywords": [
    "vite",
    "vite-plugin",
    "vite-plugin-css-export"
  ],
  "license": "MIT",
  "homepage": "https://github.com/shixuanhong/vite-plugin-css-export#readme",
  "bugs": {
    "url": "https://github.com/shixuanhong/vite-plugin-css-export/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shixuanhong/vite-plugin-css-export.git"
  },
  "scripts": {
    "dev": "vite example",
    "build": "vue-tsc --noEmit && vite build example",
    "build:lib": "unbuild",
    "preview": "vite preview example",
    "test": "jest test"
  },
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "client.d.ts"
  ],
  "devDependencies": {
    "@types/acorn": "^4.0.6",
    "@types/escodegen": "^0.0.7",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.179",
    "@types/node": "^17.0.12",
    "@vitejs/plugin-vue": "^2.0.0",
    "jest": "^27.5.1",
    "less": "^4.1.2",
    "lodash": "^4.17.21",
    "rollup": "^2.67.0",
    "sass": "^1.49.0",
    "stylus": "^0.56.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.4",
    "unbuild": "^0.6.7",
    "vite": "^2.7.10",
    "vite-plugin-inspect": "^0.3.13",
    "vue": "^3.2.28",
    "vue-tsc": "^0.29.8"
  },
  "peerDependencies": {
    "vite": "^2.0.0"
  },
  "dependencies": {
    "@rollup/pluginutils": "^4.1.2",
    "acorn-walk": "^8.2.0",
    "escodegen": "^2.0.0",
    "postcss": "^8.4.5"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@6.0.0+"
}
