{
  "name": "vite-plugin-css-export",
  "author": {
    "name": "shixuanhong"
  },
  "description": "A Vite plugin for sharing variables between Javascript and CSS (or Sass, Less, etc.)",
  "version": "3.0.0-beta.1",
  "keywords": [
    "vite",
    "vite-plugin",
    "vite-plugin-css-export"
  ],
  "license": "MIT",
  "homepage": "https://github.com/shixuanhong/vite-plugin-css-export#readme",
  "bugs": {
    "url": "https://github.com/shixuanhong/vite-plugin-css-export/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shixuanhong/vite-plugin-css-export.git"
  },
  "scripts": {
    "dev": "vite example/vue",
    "build": "vite build example/vue",
    "build:lib": "unbuild",
    "preview": "vite preview example/vue",
    "test": "jest test",
    "format": "prettier --write .",
    "publish:lib": "unbuild && npm publish"
  },
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "client.d.ts"
  ],
  "devDependencies": {
    "@types/estree": "^1.0.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^20.11.5",
    "@vitejs/plugin-vue": "^5.0.3",
    "jest": "^27.5.1",
    "prettier": "^3.2.4",
    "rollup": "^4.17.0",
    "sass": "^1.49.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.4",
    "unbuild": "^0.6.7",
    "vite": "5.0.12",
    "vite-plugin-inspect": "^0.8.1",
    "vue": "^3.2.28",
    "vue-tsc": "^0.29.8"
  },
  "peerDependencies": {
    "vite": ">=5.0.0 <6.0.0"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.1.0",
    "acorn-walk": "^8.3.2",
    "escodegen": "^2.1.0",
    "postcss": "^8.4.33"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@8.0.0+"
}
